# Sample .env file with Cloudflare R2 configuration
# Existing AWS S3 configuration (keep for reference during migration)
AWS_REGION=your-aws-region
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
S3_BUCKET=your-s3-bucket

# Cloudflare R2 configuration
CLOUDFLARE_R2_ENDPOINT=https://youraccountid.r2.cloudflarestorage.com
CLOUDFLARE_R2_ACCESS_KEY_ID=your-r2-access-key
CLOUDFLARE_R2_SECRET_ACCESS_KEY=your-r2-secret-key
CLOUDFLARE_R2_BUCKET=your-r2-bucket
# If you've set up custom domain or public access for your bucket
CLOUDFLARE_R2_PUBLIC_URL=https://your-public-bucket-domain.com

# Other existing environment variables
FEEDURLS=https://example.com/feed1|https://example.com/feed2